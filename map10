               CTRL=(FREEKB),                                          C
               LANG=COBOL,                                             C
               MAPATTS=(HILIGHT,COLOR),                                C
               MODE=INOUT,                                             C
               STORAGE=AUTO,                                           C
               TIOAPFX=YES
******LOGIN MAP*********************************
