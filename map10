         DFHMDF ATTRB=(PROT,BRT),                                      C
               INITIAL='LOGIN PAGE  ',                                 C
               LENGTH=12,                                              C
               POS=(1,25)
         DFHMDF ATTRB=(PROT,BRT),                                      C
               INITIAL='DATE :',                                       C
               LENGTH=6,                                               C
               POS=(1,60)
DATE01   DFHMDF ATTRB=(PROT,BRT),                                      C
               INITIAL='DD/MM/CCYY',                                   C
               LENGTH=10,                                              C
               POS=(1,67)
         DFHMDF ATTRB=(PROT,BRT),                                      C
               INITIAL='TIME :',                                       C
               LENGTH=6,                                               C
               POS=(2,60)
TIME01   DFHMDF ATTRB=(PROT,BRT),                                      C
               INITIAL='HH:MM:SS',                                     C
               LENGTH=8,                                               C
               POS=(2,67)
         DFHMDF ATTRB=(PROT,BRT),                                      C
               INITIAL='USER NAME :',                                  C
               LENGTH=11,                                              C
               POS=(9,10)
UNAME    DFHMDF ATTRB=(UNPROT,BRT,FSET),                               C
               INITIAL='___',                                          C
               LENGTH=3,                                               C
               POS=(9,23)
